{
  "name": "reactwolclient",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dotenv": "^0.1.3",
    "react-hook-form": "^7.25.3",
    "react-scripts": "5.0.0",
    "wake_on_lan": "^1.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set NODE_ENV=production && node app.js",
    "dev": "set NODE_ENV=development && concurrently \"npm:frontend\" \"npm:backend\" ",
    "frontend": "react-dotenv && react-scripts start",
    "backend": "nodemon app.js",
    "build": "react-dotenv && react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "http-proxy-middleware": "^2.0.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18"
  },
  "react-dotenv": {
    "whitelist": [
      "API_ENDPOINT"
    ]
  }
}
